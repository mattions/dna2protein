{
  "id": "https://api.sbgenomics.com/v2/apps/mmattioni/dna2protein/translate/16/raw/",
  "class": "CommandLineTool",
  "label": "translate",
  "description": "Transcribe the provided mRNA into a peptide",
  "requirements": [],
  "inputs": [
    {
      "inputBinding": {
        "sbg:cmdInclude": true,
        "separate": true,
        "position": 1
      },
      "id": "#rna",
      "type": [
        "File"
      ]
    },
    {
      "description": "If passed, translate will run in verbose mode",
