      "class": "sbg:MemRequirement",
      "value": 1000
    },
    {
      "class": "DockerRequirement",
      "dockerImageId": "",
      "dockerPull": "sevenbridges/dna2protein:0.5.3"
    }
  ],
  "baseCommand": [
    "translate.py"
  ],
  "stdin": "",
  "stdout": "translate.log",
  "successCodes": [],
