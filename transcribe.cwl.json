{
  "id": "https://api.sbgenomics.com/v2/apps/mmattioni/dna2protein/transcribe/12/raw/",
  "class": "CommandLineTool",
  "label": "transcribe",
  "description": "Translates a DNA input test into a RNA",
  "requirements": [],
  "inputs": [
    {
      "label": "Input DNA to transcribe",
      "inputBinding": {
        "sbg:cmdInclude": true,
        "separate": true,
        "position": 1
      },
      "id": "#dna",
      "type": [
        "File"
      ]
    },
    {
      "description": "If passed, transcribe will run in verbose mode",
      "inputBinding": {
        "sbg:cmdInclude": true,
        "separate": true,
        "position": 2,
        "prefix": "--verbose"
      },
      "id": "#verbose_transcribe",
      "type": [
        "null",
        "boolean"
      ]
    }
  ],
  "outputs": [
    {
      "type": [
        "File"
      ],
      "id": "#rna",
      "outputBinding": {
        "glob": "rna.txt"
      }
    },
    {
      "type": [
        "null",
        "File"
      ],
      "id": "#log",
      "outputBinding": {
        "glob": "transcribe.log"
      }
    }
  ],
  "hints": [
    {
      "class": "sbg:CPURequirement",
      "value": 1
    },
    {
      "class": "sbg:MemRequirement",
      "value": 1000
    },
    {
      "class": "DockerRequirement",
      "dockerImageId": "",
      "dockerPull": "sevenbridges/dna2protein:0.5.4"
    }
  ],
  "baseCommand": [
    "transcribe.py"
  ],
  "stdin": "",
  "stdout": "transcribe.log",
  "successCodes": [],
  "temporaryFailCodes": [],
  "arguments": [],
  "sbg:id": "mmattioni/dna2protein/transcribe/12",
  "sbg:toolAuthor": "Seven Bridges",
  "sbg:contributors": [
